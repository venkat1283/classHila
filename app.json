{
  "expo": {
    "name": "Classhila",
    "slug": "Classhila",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "backgroundColor": "#FF731F",
      "resizeMode": "cover",
      "image": "./assets/images/splash.png"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "newArchEnabled": false,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.classhiledu.classhila"
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "expo-web-browser",
      "expo-video",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash.png",
          "resizeMode": "cover",
          "backgroundColor": "#FF731F"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "63fbe2b7-11c0-46b9-b594-3b59a187c9ac"
      }
    },
    "android": {
      "package": "com.classhilaedu.classhila",
      "navigationBar": {
        "visible": "hidden",
        "mode": "leanback"
      }
    }
  }
}
